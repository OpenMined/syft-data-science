{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Dataset",
  "type": "object",
  "properties": {
    "name": {
      "title": "Name",
      "description": "Name of the dataset.",
      "type": "string"
    },
    "description": {
      "title": "Description",
      "description": "Description of the dataset.",
      "type": "string"
    },
    "mock_path": {
      "title": "Mock Path",
      "description": "Mock path of the dataset.",
      "type": "string"
    },
    "private_path": {
      "title": "Private Path",
      "description": "Private path of the dataset.",
      "type": "string"
    },
    "tags": {
      "title": "Tags",
      "description": "Tags for the dataset.",
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": ["name", "description", "tags", "private_path", "mock_path"]
}
